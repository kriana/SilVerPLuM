{
    "id": "lookup-anything",
    "name": "Lookup Anything",
    "author": "Pathoschild",
    "license": "LGPL",
    "url": "https://github.com/Pathoschild/LookupAnything",
    "version": "1.5",
    "requires": ["stardewvalley=1.1.1", "smapi=0.40.1.1"],
    "categories" : ["User Interface"],
    "content": {
        "content": {
            "name": "Lookup Anything",
            "description": "Contains the necessary mod data.",
            "default": true,
            "platforms": ["linux", "mac", "windows"],
            "priority": 0,
            "pipeline": "compile-dll",
            "nuget-restore": true,
            "references": {
                "StardewModdingAPI" : "smapi://@smapi/StardewModdingAPI.exe"
                },
            "build-parameters" : {
                "windows" : ["/p:Configuration=Release"],
                "linux" : ["/p:Configuration=Release"],
                "mac" : ["/p:Configuration=Release"]
                },
            "location": "file",
            "installables": {
                "LookupAnything/bin/Release/LookupAnything.dll": "Mods/LookupAnything/LookupAnything.dll",                
                "LookupAnything/data.json": "Mods/LookupAnything/data.json",
                "LookupAnything/manifest.json": "Mods/LookupAnything/manifest.json"
            }            
        }
    }
}
